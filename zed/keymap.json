[
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-h": "workspace::ActivatePaneLeft",
      "ctrl-l": "workspace::ActivatePaneRight",
      "ctrl-k": "workspace::ActivatePaneUp",
      "ctrl-j": "workspace::ActivatePaneDown",
      "ctrl-cmd-h": "workspace::SwapPaneLeft",
      "ctrl-cmd-l": "workspace::SwapPaneRight",
      "ctrl-cmd-k": "workspace::SwapPaneUp",
      "ctrl-cmd-j": "workspace::SwapPaneDown",
      "cmd-k h": "workspace::ToggleLeftDock",
      "cmd-k l": "workspace::ToggleRightDock"
    }
  },
  {
    "context": "",
    "bindings": {
      "J": "menu::SelectNext",
      "K": "menu::SelectPrevious"
    }
  },
  {
    "context": "Editor && vim_mode == normal && !menu && !BufferSearchBar",
    "bindings": {
      "K": "editor::Hover",
      "L": "pane::ActivateNextItem",
      "H": "pane::ActivatePreviousItem",
      "space w": "workspace::Save",
      "space c": "pane::CloseActiveItem",
      "ctrl-h": "workspace::ActivatePaneLeft",
      "ctrl-l": "workspace::ActivatePaneRight",
      "ctrl-k": "workspace::ActivatePaneUp",
      "ctrl-j": "workspace::ActivatePaneDown",
      "ctrl-cmd-h": "workspace::SwapPaneLeft",
      "ctrl-cmd-l": "workspace::SwapPaneRight",
      "ctrl-cmd-k": "workspace::SwapPaneUp",
      "ctrl-cmd-j": "workspace::SwapPaneDown",
      "ctrl-shift-l": "pane::SplitRight",
      "ctrl-shift-h": "pane::SplitLeft",
      "ctrl-shift-j": "pane::SplitDown",
      "ctrl-shift-k": "pane::SplitUp",
      "space g b": "git::Blame",
      "space g h": "editor::ToggleSelectedDiffHunks",
      "space g r": "git::Restore",
      "space g s": "git::ToggleStaged",
      "space g c": "git::ExpandCommitEditor",
      "space /": "editor::ToggleComments",
      "space c a": "editor::ToggleCodeActions",
      "space f": "editor::Format",
      "space r": "editor::Rename",
      "space d": "diagnostics::Deploy",
      "space s f": "file_finder::Toggle",
      "space o": "outline::Toggle"
    }
  },
  {
    "context": "vim_mode == normal || vim_mode == visual",
    "bindings": {
      "s": "vim::PushSneak",
      "shift-s": "vim::PushSneakBackward"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "L": "pane::ActivateNextItem",
      "H": "pane::ActivatePreviousItem",
      "ctrl-cmd-h": "workspace::SwapPaneLeft",
      "ctrl-cmd-l": "workspace::SwapPaneRight",
      "ctrl-cmd-k": "workspace::SwapPaneUp",
      "ctrl-cmd-j": "workspace::SwapPaneDown",
      "ctrl-shift-l": "pane::SplitRight",
      "ctrl-shift-h": "pane::SplitLeft",
      "ctrl-shift-j": "pane::SplitDown",
      "ctrl-shift-k": "pane::SplitUp"
    }
  }
]
